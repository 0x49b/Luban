{
    "esnext": true,
    "preset": "airbnb",
    "fileExtensions": [".js", ".jsx"],
    "maximumLineLength": {
        "value": 160,
        "allExcept": ["comments", "regex"]
    },
    "validateIndentation": 4,
    "requirePaddingNewLinesAfterBlocks": false,
    "requireTrailingComma": false,
    "excludeFiles": [
        "node_modules/**"
    ]
}
