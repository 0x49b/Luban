:global {
    [data-ns="widgets/grbl"] {
        .widget-content {
            position: relative;
            padding: 10px;

            .row {
                margin-bottom: 5px;
            }
            .row:last-child {
                margin-bottom: 0;
            }

            .well {
                text-overflow: ellipsis;
                overflow: hidden;
                white-space: nowrap;
            }
            .well-xs {
                 padding: 0 5px;
                 border-radius: 3px;
                 margin-bottom: 0;
                 line-height: 18px;
            }
        }
    }
}

.toolbar {
    margin-bottom: 10px;
}
.panel {
    margin-bottom: 10px;
    &.last {
        margin-bottom: 0;
    }
}
