{
  "name": "cncjs",
  "version": "1.1.0",
  "description": "A web-based CNC milling controller for the Arduino running GRBL",
  "homepage": "https://github.com/cheton/cnc",
  "author": "Cheton Wu <cheton@gmail.com>",
  "main": "./dist/cnc/cnc.js",
  "bin": {
    "cnc": "./bin/cnc"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cheton/cnc.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10.x"
  },
  "preferGlobal": true,
  "scripts": {
    "prepublish": "npm run clean && npm run lint && npm run build",
    "prebuild": "gulp pkg-sync; bash -c 'scripts/prebuild.sh'",
    "prebuild-latest": "npm run prebuild; cd dist/cnc; npm version ${npm_package_version}-latest",
    "prebuild-dev": "npm run prebuild",
    "prebuild-prod": "npm run prebuild",
    "build": "gulp prod",
    "build-latest": "gulp prod",
    "build-dev": " gulp dev",
    "build-prod": "gulp prod",
    "clean": "bash -c 'rm -rf ./dist ./output'",
    "electron-packager:linux-ia32": "bash -c 'PLATFORM=linux ARCH=ia32 scripts/electron-packger.sh'",
    "electron-packager:linux-x64": "bash -c 'PLATFORM=linux ARCH=x64 scripts/electron-packger.sh'",
    "electron-packager:darwin-x64": "bash -c 'PLATFORM=darwin ARCH=x64 scripts/electron-packger.sh'",
    "electron-packager:win32-ia32": "bash -c 'PLATFORM=win32 ARCH=ia32 scripts/electron-packger.sh'",
    "electron-packager:win32-x64": "bash -c 'PLATFORM=win32 ARCH=x64 scripts/electron-packager.sh'",
    "electron-builder:linux-ia32": "bash -c 'PLATFORM=linux ARCH=ia32 scripts/electron-builder.sh'",
    "electron-builder:linux-x64": "bash -c 'PLATFORM=linux ARCH=x64 scripts/electron-builder.sh'",
    "electron-builder:darwin-x64": "bash -c 'PLATFORM=darwin ARCH=x64 scripts/electron-builder.sh'",
    "electron-builder:win32-ia32": "bash -c 'PLATFORM=win32 ARCH=ia32 scripts/electron-builder.sh'",
    "electron-builder:win32-x64": "bash -c 'PLATFORM=win32 ARCH=x64 scripts/electron-builder.sh'",
    "start": "./bin/cnc",
    "start-electron": "electron ./dist/cnc/main",
    "predev": "bash -c 'watchpack-ignore-node-modules.sh'",
    "dev": "npm run build-dev && NODE_ENV=\"development\" ./bin/cnc -vv",
    "prod": "npm run build-prod && NODE_ENV=\"production\" ./bin/cnc",
    "lint": "npm run eslint && npm run stylint",
    "eslint": "eslint *.js src bin gulp test",
    "stylint": "stylint src/web",
    "test": "node babel-tap --coverage test/*.js",
    "coveralls": "node babel-tap --coverage --coverage-report=text-lcov test/*.js | node_modules/.bin/coveralls"
  },
  "build": {
    "app-bundle-id": "cnc",
    "app-category-type": "public.app-category.productivity",
    "asar": false,
    "osx": {
      "title": "CNC",
      "icon": "build/icon.icns",
      "background": "build/background.png",
      "icon-size": 80,
      "contents": [
        {
          "x": 448,
          "y": 344,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 192,
          "y": 344,
          "type": "file"
        }
      ]
    }
  },
  "directories": {
    "buildResources": "build",
    "output": "output",
    "app": "dist/cnc"
  },
  "keywords": [
    "cnc",
    "gcode",
    "grbl",
    "arduino",
    "raspberry-pi",
    "react",
    "socket.io",
    "webappengine"
  ],
  "dependencies": {
    "async": "~1.5.2",
    "body-parser": "~1.15.0",
    "bootstrap": "~3.3.6",
    "chalk": "~1.1.3",
    "classnames": "~2.2.4",
    "colornames": "~1.1.1",
    "commander": "~2.9.0",
    "compression": "~1.6.1",
    "connect-multiparty": "~2.0.0",
    "connect-restreamer": "~1.0.3",
    "consolidate": "~0.14.1",
    "cookie-parser": "~1.4.1",
    "del": "~2.2.0",
    "errorhandler": "~1.4.3",
    "express": "~4.13.4",
    "express-session": "~1.13.0",
    "font-awesome": "~4.6.1",
    "gcode-interpreter": "~0.9.3",
    "gcode-parser": "~0.8.2",
    "gcode-toolpath": "~0.6.2",
    "hogan.js": "~3.0.2",
    "i18next": "~2.5.1",
    "i18next-browser-languagedetector": "~0.3.0",
    "i18next-express-middleware": "~1.0.1",
    "i18next-node-fs-backend": "~0.1.2",
    "i18next-xhr-backend": "~0.5.3",
    "jsuri": "~1.3.1",
    "lodash": "~4.11.1",
    "method-override": "~2.3.5",
    "moment": "~2.13.0",
    "morgan": "~1.7.0",
    "mousetrap": "~1.5.3",
    "multiparty": "~4.1.2",
    "normalize.css": "~4.1.1",
    "pubsub-js": "~1.5.3",
    "range_check": "~1.2.0",
    "rc-slider": "~3.6.0",
    "rc-switch": "~1.4.2",
    "react": "~15.0.2",
    "react-addons-shallow-compare": "~15.0.2",
    "react-addons-update": "~15.0.2",
    "react-bootstrap": "~0.29.3",
    "react-dom": "~15.0.2",
    "react-dropzone": "~3.4.0",
    "react-infinite": "~0.9.2",
    "react-router": "~2.4.0",
    "react-select": "~1.0.0-beta12",
    "react-sortablejs": "~1.0.0",
    "react-virtualized": "~6.3.1",
    "serialport": "~3.0.1",
    "serve-favicon": "~2.3.0",
    "serve-static": "~1.10.2",
    "session-file-store": "~0.2.0",
    "sha1": "~1.1.1",
    "socket.io": "~1.4.5",
    "sortablejs": "~1.4.2",
    "stacktrace-js": "~1.1.1",
    "superagent": "~1.8.0-beta.2",
    "three": "~0.76.1",
    "webappengine": "~0.8.4",
    "winston": "~2.2.0"
  },
  "devDependencies": {
    "babel-cli": "~6.7.5",
    "babel-core": "~6.7.2",
    "babel-eslint": "~6.0.0-beta.6",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-react-hmre": "~1.1.1",
    "babel-preset-stage-0": "~6.5.0",
    "coveralls": "~2.11.8",
    "css-loader": "~0.23.1",
    "electron-builder": "~3.16.0",
    "electron-packager": "~7.0.0",
    "electron-prebuilt": "~0.37.8",
    "electron-rebuild": "~1.1.3",
    "eslint": "~2.9.0",
    "eslint-config-airbnb": "~8.0.0",
    "eslint-loader": "~1.3.0",
    "eslint-plugin-import": "~1.6.1",
    "eslint-plugin-jsx-a11y": "~1.0.4",
    "eslint-plugin-react": "~5.0.1",
    "eventsource-polyfill": "~0.9.6",
    "file-loader": "~0.8.5",
    "gulp": "~3.9.0",
    "gulp-babel": "~6.1.2",
    "gulp-util": "~3.0.3",
    "i18next-scanner": "~1.2.1",
    "json-loader": "~0.5.4",
    "nib": "~1.1.0",
    "require-dir": "~0.3.0",
    "run-sequence": "~1.1.5",
    "style-loader": "~0.13.0",
    "stylint": "~1.3.6",
    "stylint-loader": "~1.0.0",
    "stylus": "~0.54.2",
    "stylus-loader": "~2.0.0",
    "tap": "~5.7.0",
    "text-table": "~0.2.0",
    "transform-loader": "~0.2.3",
    "url-loader": "~0.5.7",
    "webpack": "~1.13.0",
    "webpack-dev-middleware": "~1.6.1",
    "webpack-hot-middleware": "~2.10.0"
  }
}
