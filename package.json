    "run-sequence": "~1.1.5",
    "style-loader": "~0.13.0",
    "stylint": "~1.3.6",
    "stylint-loader": "~1.0.0",
    "stylus": "~0.54.2",
    "stylus-loader": "~2.0.0",
    "tap": "~5.7.0",
    "text-table": "~0.2.0",
    "transform-loader": "~0.2.3",
    "url-loader": "~0.5.7",
    "webpack": "~1.12.15",
    "webpack-dev-middleware": "~1.6.1",
    "webpack-hot-middleware": "~2.10.0"
  },
  "browser": {}
}
