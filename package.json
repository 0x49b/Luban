    "eslint": "~2.2.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.0",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^0.8.5",
