{
  "name": "cncjs",
  "version": "0.15.8",
  "description": "CNC.js is a web-based CNC milling controller for the Arduino running GRBL",
  "homepage": "https://github.com/cheton/cnc.js",
  "author": "Cheton Wu <cheton@gmail.com>",
  "main": "./dist/app/index.js",
  "bin": {
    "cnc": "./bin/cnc"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cheton/cnc.js.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10.x"
  },
  "preferGlobal": true,
  "scripts": {
    "prepublish": "npm run lint && npm run build && npm test",
    "build": "npm run build-prod",
    "build-dev": "gulp dev",
    "build-prod": "gulp prod",
    "start": "./bin/cnc",
    "dev": "npm run build-dev && NODE_ENV=\"development\" ./bin/cnc -vv",
    "prod": "npm run build-prod && NODE_ENV=\"production\" ./bin/cnc",
    "lint": "npm run eslint",
    "eslint": "eslint *.js src bin gulp test",
    "test": "./babel-tap --coverage test/*.js",
    "coveralls": "./babel-tap --coverage --coverage-report=text-lcov test/*.js | node_modules/.bin/coveralls"
  },
  "keywords": [
    "cnc",
    "gcode",
    "grbl",
    "arduino",
    "raspberry-pi",
    "react",
    "socket.io",
    "webappengine"
  ],
  "dependencies": {
    "async": "^1.5.2",
    "body-parser": "^1.15.0",
    "bootstrap": "^3.3.6",
    "classnames": "^2.2.1",
    "colornames": "^1.1.1",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
