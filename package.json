    "del": "^2.2.0",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "font-awesome": "^4.5.0",
    "gcode-interpreter": "^0.9.3",
    "gcode-parser": "^0.8.2",
    "gcode-toolpath": "^0.6.2",
    "hogan.js": "^3.0.2",
    "i18next": "^2.2.0",
    "i18next-browser-languagedetector": "^0.1.0",
    "i18next-express-middleware": "^0.3.2",
    "i18next-node-fs-backend": "0.0.6",
    "i18next-xhr-backend": "^0.4.0",
