{
  "name": "cncjs",
  "version": "1.1.0",
  "description": "A web-based CNC milling controller for the Arduino running GRBL",
  "homepage": "https://github.com/cheton/cnc",
  "author": "Cheton Wu <cheton@gmail.com>",
  "main": "./dist/app/index.js",
  "bin": {
    "cnc": "./bin/cnc"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cheton/cnc.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10.x"
  },
  "preferGlobal": true,
  "scripts": {
    "prepublish": "npm run lint && npm test && npm run build",
    "build": "npm run build-prod",
    "build-dev": "gulp dev",
    "build-prod": "gulp prod",
    "start": "./bin/cnc",
    "predev": "./watchpack-ignore-node-modules.sh",
    "dev": "npm run build-dev && NODE_ENV=\"development\" ./bin/cnc -vv",
    "prod": "npm run build-prod && NODE_ENV=\"production\" ./bin/cnc",
    "lint": "npm run eslint && npm run stylint",
    "eslint": "eslint *.js src bin gulp test",
    "stylint": "stylint src/web",
    "test": "./babel-tap --coverage test/*.js",
    "coveralls": "./babel-tap --coverage --coverage-report=text-lcov test/*.js | node_modules/.bin/coveralls"
  },
  "keywords": [
    "cnc",
    "gcode",
    "grbl",
    "arduino",
    "raspberry-pi",
    "react",
    "socket.io",
    "webappengine"
  ],
  "dependencies": {
    "async": "~1.5.2",
    "body-parser": "~1.15.0",
    "bootstrap": "~3.3.6",
    "chalk": "~1.1.3",
    "classnames": "~2.2.1",
    "colornames": "~1.1.1",
    "commander": "~2.9.0",
    "compression": "~1.6.1",
    "connect-multiparty": "~2.0.0",
    "connect-restreamer": "~1.0.3",
    "consolidate": "~0.14.0",
    "cookie-parser": "~1.4.1",
    "del": "~2.2.0",
    "errorhandler": "~1.4.3",
    "express": "~4.13.4",
    "express-session": "~1.13.0",
    "font-awesome": "~4.6.1",
    "gcode-interpreter": "~0.9.3",
    "gcode-parser": "~0.8.2",
    "gcode-toolpath": "~0.6.2",
    "hogan.js": "~3.0.2",
    "i18next": "~2.5.1",
    "i18next-browser-languagedetector": "~0.2.2",
    "i18next-express-middleware": "~0.4.0",
    "i18next-node-fs-backend": "~0.1.2",
    "i18next-xhr-backend": "~0.5.3",
    "jsuri": "~1.3.1",
    "lodash": "~4.11.1",
    "method-override": "~2.3.5",
    "moment": "~2.13.0",
    "morgan": "~1.7.0",
    "mousetrap": "~1.5.3",
    "multiparty": "~4.1.2",
    "normalize.css": "~4.1.1",
    "pubsub-js": "~1.5.3",
    "range_check": "~1.2.0",
    "rc-slider": "~3.6.0",
    "rc-switch": "~1.4.1",
    "react": "~15.0.1",
    "react-addons-shallow-compare": "~15.0.1",
    "react-addons-update": "~15.0.1",
    "react-bootstrap": "~0.29.0",
    "react-dom": "~15.0.1",
    "react-dropzone": "~3.4.0",
    "react-infinite": "~0.9.1",
    "react-router": "~2.3.0",
    "react-select": "~1.0.0-beta12",
    "react-sortablejs": "~1.0.0",
    "react-virtualized": "~6.2.0",
    "serialport": "~2.1.0",
    "serve-favicon": "~2.3.0",
    "serve-static": "~1.10.2",
    "session-file-store": "~0.2.0",
    "sha1": "~1.1.1",
    "socket.io": "~1.4.5",
    "sortablejs": "~1.4.2",
    "stacktrace-js": "~1.1.0",
    "superagent": "~1.8.0-beta.2",
    "three": "~0.75.0",
    "webappengine": "~0.8.0",
    "winston": "~2.2.0"
  },
  "devDependencies": {
    "babel-cli": "~6.7.5",
    "babel-core": "~6.7.2",
    "babel-eslint": "~6.0.0-beta.6",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-react-hmre": "~1.1.1",
    "babel-preset-stage-0": "~6.5.0",
    "coveralls": "~2.11.8",
    "css-loader": "~0.23.1",
    "eslint": "~2.7.0",
    "eslint-config-airbnb": "~7.0.0",
    "eslint-loader": "~1.3.0",
    "eslint-plugin-jsx-a11y": "~0.6.2",
    "eslint-plugin-react": "~4.3.0",
    "eventsource-polyfill": "~0.9.6",
    "file-loader": "~0.8.5",
    "gulp": "~3.9.0",
    "gulp-babel": "~6.1.2",
    "gulp-util": "~3.0.3",
    "i18next-scanner": "~1.1.0",
    "json-loader": "~0.5.4",
    "main-bower-files": "~2.11.1",
    "nib": "~1.1.0",
